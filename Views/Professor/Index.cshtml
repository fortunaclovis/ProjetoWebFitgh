@model IEnumerable<WebFight.Models.Professor>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Pessoa_Id)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Graduacao_Id)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Academia_Id)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Pessoa_Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Graduacao_Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Academia_Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id_Professor">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id_Professor">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id_Professor">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
